<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="fr.isen.dufaza.androidtoolbox">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/talosprinciple_rounded"
        android:label="@string/app_name"
        android:roundIcon="@drawable/talos_principle_crop_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="fr.isen.dufaza.androidtoolbox.WebServicesActivity" />
        <activity android:name="fr.isen.dufaza.androidtoolbox.PermitActivity" />
        <activity android:name="fr.isen.dufaza.androidtoolbox.StorageActivity" />
        <activity android:name="fr.isen.dufaza.androidtoolbox.FormulaireActivity" />
        <activity android:name="fr.isen.dufaza.androidtoolbox.CycleFragment2" />
        <activity android:name="fr.isen.dufaza.androidtoolbox.CycleFragment1" />
        <activity android:name="fr.isen.dufaza.androidtoolbox.LifeCycleActivity" />
        <activity
            android:name="fr.isen.dufaza.androidtoolbox.HomeActivity"
            android:label="@string/title_app1"
            android:theme="@style/AppTheme.BackGroundColorIsRed">
        </activity>
        <activity
            android:name="fr.isen.dufaza.androidtoolbox.LoginActivity"
            android:label="@string/title_app2"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>